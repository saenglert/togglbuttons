{
    "extends": "tslint:recommended",
    "rules": {
        "max-line-length": {
            "options": [120]
        },
        "arrow-parens": false,
        "new-parens": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-console": {
            "severity": "warning",
            "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
        },
        "object-literal-sort-keys": false,
        "interface-name":false
    },
    "jsRules": {
        "max-line-length": {
            "options": [120]
        }
    }
}